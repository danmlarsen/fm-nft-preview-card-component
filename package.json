{
    "scripts": {
        "cleanup": "shx rm -rf .parcel-cache dist",
        "start": "npm run cleanup && parcel src/index.html",
        "build": "parcel build src/index.html --public-url ./",
        "push": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
    },
    "devDependencies": {
        "parcel": "^2.12.0",
        "postcss": "^8.4.45",
        "push-dir": "^0.4.1",
        "shx": "^0.3.4",
        "tailwindcss": "^3.4.11"
    }
}
